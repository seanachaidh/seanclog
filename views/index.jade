extends ./heading.jade
block content
	nav.navbar.navbar-default(role='navigation')
		div.container-fluid
			div.navbar-header
				a(class='navbar-brand', href='#')
				ul.nav.navbar-nav
					li: a(href='#/tracks?access_token=' + token) Tracks
					li: a(href='#/projects?access_token=' + token) Projects
					li: a(href='#/clients?access_token=' + token) Clients
					li: a(href='#/profile') Profile
				div(class='navbar-form navbar-right', role='search')
					div(class='form-group'): input(type='text' class='form-control', placeholder='Search')
					button(type='submit' class='btn btn-default') Search!
	div.ng-view
